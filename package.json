{
  "name": "nauka",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --hot",
    "build": "npm run build:main && npm run build:ys && npm run build:krd && npm run build:yanao",
    "dev": "npm run dev:main && npm run dev:ys && npm run dev:krd && npm run dev:yanao",
    "build:main": "vue-cli-service build",
    "dev:main": "vue-cli-service build --mode development",
    "lint:main": "vue-cli-service lint",
    "watch:main": "vue-cli-service build --mode development --watch",
    "build:ys": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=./vue.config.ys.js vue-cli-service build",
    "lint:ys": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=./vue.config.ys.js vue-cli-service lint frontend-ys/components",
    "watch:ys": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=./vue.config.ys.js vue-cli-service build --mode development --watch",
    "dev:ys": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=./vue.config.ys.js vue-cli-service build --mode development",
    "build:yanao": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=./vue.config.yanao.js vue-cli-service build",
    "lint:yanao": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=./vue.config.yanao.js vue-cli-service lint frontend-yanao/components",
    "watch:yanao": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=./vue.config.yanao.js vue-cli-service build --mode development --watch",
    "dev:yanao": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=./vue.config.yanao.js vue-cli-service build --mode development",
    "build:krd": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=./vue.config.krd.js vue-cli-service build",
    "lint:krd": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=./vue.config.krd.js vue-cli-service lint frontend-krd/components",
    "watch:krd": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=./vue.config.krd.js vue-cli-service build --mode development --watch",
    "dev:krd": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=./vue.config.krd.js vue-cli-service build --mode development"
  },
  "dependencies": {
    "@xkeshi/image-compressor": "^0.5.3",
    "axios": "^0.21.0",
    "chart.js": "^2.9.4",
    "chartjs-plugin-datalabels": "^0.7.0",
    "compressorjs": "^1.0.7",
    "core-js": "^3.6.5",
    "cropperjs": "^1.5.11",
    "dayjs": "^1.11.10",
    "element-closest-polyfill": "^1.0.2",
    "gsap": "^3.6.1",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "punycode": "^2.3.1",
    "quill": "^1.3.7",
    "svg-inline-loader": "^0.8.2",
    "swiper": "^5.4.5",
    "v-body-scroll-lock": "^1.1.3",
    "v-click-outside": "^3.1.2",
    "v-mask": "^2.2.4",
    "v-tooltip": "^2.1.3",
    "vue": "^2.6.11",
    "vue-awesome-swiper": "^4.1.1",
    "vue-chartjs": "^3.5.1",
    "vue-cool-lightbox": "^2.7.3",
    "vue-dragscroll": "^2.1.3",
    "vue-moment": "^4.1.0",
    "vue-object-fit": "^1.0.6",
    "vue-rellax": "^0.2.0",
    "vue-select": "^3.11.2",
    "vue-slide-toggle": "^1.1.1",
    "vue-upload-component": "^2.8.22",
    "vue-yandex-maps": "^0.11.13",
    "vue2-smooth-scroll": "^1.5.0",
    "vue2-touch-events": "^3.2.2",
    "vuebar": "0.0.20",
    "vuelidate": "^0.7.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "^4.1.1",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/no-parsing-error": [
        2,
        {
          "invalid-first-character-of-tag-name": false
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
